getgenv().autoStrength = false;
getgenv().autoDurability = false;
getgenv().autoAgility = false;
getgenv().autoNen = false;
getgenv().areatrain = false;
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Nen Fighting Simulator", "BloodTheme")
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")
local TelePorts = Window:NewTab("Teleport")
local StrengthSec = TelePorts:NewSection("Strength")
local DurabilitySec = TelePorts:NewSection("Durability")
local AgilitySec = TelePorts:NewSection("Agility")
local NenSec = TelePorts:NewSection("Nen")
local Credit = Window:NewTab("Credits")
local CreditSec = Credit:NewSection("Credits")
CreditSec:NewLabel("Credits to velone#0857")
MainSection:NewToggle("Auto Strength", "Auto trains strength", function(v)
    getgenv().autoStrength = v
    spawn(function()
        while getgenv().autoStrength == true do
            wait()
            local args = {
                [1] = "str"
            }
            
            game:GetService("ReplicatedStorage").Remotes.train:FireServer(unpack(args))
end
end)
end)
MainSection:NewToggle("Auto Durability", "Auto trains durability", function(v)
        getgenv().autoDurability = v
        spawn(function()
            while getgenv().autoDurability == true do
                wait()
local args = {
    [1] = "dur"
}

game:GetService("ReplicatedStorage").Remotes.train:FireServer(unpack(args))

            end

    end)
end)
MainSection:NewToggle("Auto Agility", "Auto trains agility", function(v)
    getgenv().autoAgility = v
    spawn(function()
        while getgenv().autoAgility == true do
            wait()
local args = {
    [1] = "agi"
}

game:GetService("ReplicatedStorage").Remotes.train:FireServer(unpack(args))

        end
end)
end)
MainSection:NewToggle("Auto Nen", "Auto trains Nen", function(v)
