    while getgenv().autoNen == true do
        wait()
local args = {
    [1] = "nen"
}

game:GetService("ReplicatedStorage").Remotes.train:FireServer(unpack(args))

    end
end)
end)

StrengthSec:NewButton("Gym(No Req Str)", "5x Strength", function()
    function teleportTO(placeCFrame)
        local player = game.Players.LocalPlayer;
        if player.Character then
        player.Character.HumanoidRootPart.CFrame = placeCFrame;
        end
    end
    teleportTO(game:GetService("Workspace").Zones.GymZone.CFrame)
    
    function teleportZone(zone)
        if game:GetService("Workspace").Zones:FindFirstChild(zone) then
            teleportTO(game:GetService("Workspace").Zones[zone].GymZone.CFrame)
    end
    end
    
    teleportZone('GymZone')
end)
